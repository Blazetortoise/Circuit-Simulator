#include <iostream>
#include "System.h"
using namespace std;

class Resistor : public System {
public:
      int res;
      double voltage;
      double current;
      
      Resistor(string , int n1, int n2, double res): {}



  void print(){
      cout << "Resistor with Resistance: " << res << " Current: " << current << " Voltage: " << voltage<<endl;


    }
  
      double getValue() {return res; }
  
};
